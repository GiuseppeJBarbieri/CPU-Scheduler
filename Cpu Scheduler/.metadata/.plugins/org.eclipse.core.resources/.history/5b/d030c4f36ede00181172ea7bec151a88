package model;

public class Process_Object {
	private int processNum;
	private int burstTime;
	private int executionTime;
	private int waitTime;
	private int avgTurnAroundTime;

	public Process_Object(int processNum, int burstTime, int executionTime, int waitTime, int avgTurnAroundTime) {
		super();
		this.processNum = processNum;
		this.burstTime = burstTime;
		this.executionTime = executionTime;
		this.waitTime = waitTime;
		this.avgTurnAroundTime = avgTurnAroundTime;
	}
	
	public Process_Object(int processNum, int waitTime, int avgTurnAroundTime, int arrivalTime) {
		super();
		this.processNum = processNum;
		this.waitTime = waitTime;
		this.avgTurnAroundTime = avgTurnAroundTime;
	}

	public int getProcessNum() {
		return processNum;
	}

	public void setProcessNum(int processNum) {
		this.processNum = processNum;
	}

	public int getBurstTime() {
		return burstTime;
	}

	public void setBurstTime(int burstTime) {
		this.burstTime = burstTime;
	}

	public int getExecutionTime() {
		return executionTime;
	}

	public void setExecutionTime(int executionTime) {
		this.executionTime = executionTime;
	}

	public int getAvgWaitTime() {
		return waitTime;
	}

	public void setAvgWaitTime(int avgWaitTime) {
		this.waitTime = avgWaitTime;
	}

	public int getAvgTurnAroundTime() {
		return avgTurnAroundTime;
	}

	public void setAvgTurnAroundTime(int avgTurnAroundTime) {
		this.avgTurnAroundTime = avgTurnAroundTime;
	}

}
